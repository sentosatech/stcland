{
  "name": "@stcland/components",
  "version": "0.0.3",
  "type": "module",
  "main": "./dist/index.es.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./withStyles": {
      "import": "./dist/withStyles.es.js",
      "require": "./dist/withStyles.cjs.js",
      "types": "./dist/withStyles.d.ts"
    },
    "./icon": {
      "import": "./dist/icon.es.js",
      "require": "./dist/icon.cjs.js",
      "types": "./dist/icon.d.ts"
    },
    "./icon/withStyles": {
      "import": "./dist/iconWithStyles.es.js",
      "require": "./dist/iconWithStyles.cjs.js",
      "types": "./dist/iconWithStyles.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext .js,.ts 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "ts-check": "tsc --noEmit --skipLibCheck",
    "preview": "vite preview"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.5",
    "@stcland/utils": "workspace:^",
    "@tanstack/react-table": "^8.20.5",
    "d3-interpolate": "^3.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@tailwindcss/typography": "^0.5.14",
    "@types/d3-interpolate": "^3.0.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vite-plugin-dts": "^4.0.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
